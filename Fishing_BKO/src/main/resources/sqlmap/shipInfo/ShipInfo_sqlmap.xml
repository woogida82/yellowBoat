            FROM (
                SELECT ROWNUM AS RNUM, Z.*
                    FROM (                 
                        SELECT @RNUM := @RNUM + 1 AS ROWNUM, A.*
                            FROM (     
                                    SELECT 
                                        SHIP_ID,
                                        SHIP_REG_NO,
                                        USER_ID,
                                        SHIP_CD,
                                        SHIP_NM,
                                        SHIP_WEIGHT,
                                        BOARDING_QUOTA,
                                        MAIN_FISHING_POINT,
                                        OUTPORT,
                                        SERVICE_CD,
                                        STATUS_CD,
                                        ORG_MAIN_IMG_PATH,
                                        ORG_MAIN_IMG_NM,
                                        THUM_MAIN_IMG_PATH,
                                        THUM_MAIN_IMG_SIZE_CD,
                                        ORG_01_IMG_PATH,
                                        ORG_01_IMG_NM,
                                        THUM_01_IMG_PATH,
                                        THUM_01_IMG_SIZE_CD,
                                        ORG_02_IMG_PATH,
                                        ORG_02_IMG_NM,
                                        THUM_02_IMG_PATH,
                                        THUM_02_IMG_SIZE_CD,
                                        ORG_03_IMG_PATH,
                                        ORG_03_IMG_NM,
                                        THUM_03_IMG_PATH,
                                        THUM_03_IMG_SIZE_CD,
                                        ORG_04_IMG_PATH,
                                        ORG_04_IMG_NM,
                                        THUM_04_IMG_PATH,
                                        THUM_04_IMG_SIZE_CD,
                                        ORG_05_IMG_PATH,
                                        ORG_05_IMG_NM,
                                        THUM_05_IMG_PATH,
                                        THUM_05_IMG_SIZE_CD,
                                        ORG_06_IMG_PATH,
                                        ORG_06_IMG_NM,
                                        THUM_06_IMG_PATH,
                                        THUM_06_IMG_SIZE_CD,
                                        ORG_07_IMG_PATH,
                                        ORG_07_IMG_NM,
                                        THUM_07_IMG_PATH,
                                        THUM_07_IMG_SIZE_CD,
                                        ORG_08_IMG_PATH,
                                        ORG_08_IMG_NM,
                                        THUM_08_IMG_PATH,
                                        THUM_08_IMG_SIZE_CD,
                                        ORG_09_IMG_PATH,
                                        ORG_09_IMG_NM,
                                        THUM_09_IMG_PATH,
                                        THUM_09_IMG_SIZE_CD,
                                        ORG_10_IMG_PATH,
                                        ORG_10_IMG_NM,
                                        THUM_10_IMG_PATH,
                                        THUM_10_IMG_SIZE_CD,
                                        DEL_YN
                                        ,(SELECT A.USER_NM FROM T_USER A WHERE A.USER_ID = T_SHIP_INFO.USER_ID) USER_NM
                                        ,(SELECT A.USER_NM FROM T_USER A WHERE A.USER_ID = T_SHIP_INFO.UPDATE_ID) UPDATE_ID
                                        ,DATE_FORMAT( UPDATE_TIME,'%Y.%m.%d %H:%i:%s' ) UPDATE_TIME
                                        ,(SELECT A.USER_NM FROM T_USER A WHERE A.USER_ID = T_SHIP_INFO.CREATE_ID) CREATE_ID
                                        ,DATE_FORMAT( CREATE_TIME,'%Y.%m.%d %H:%i:%s' ) CREATE_TIME
                                      FROM T_SHIP_INFO
                                     WHERE 1 = 1
                                    <include refid="shipInfoWhere"/>
                                    ORDER BY CREATE_TIME DESC
                                ) A
                                , (SELECT @RNUM := 0) B
                        ) Z
                    WHERE ROWNUM <![CDATA[ <= ${end} ]]>
                ) MainSQL
            WHERE RNUM <![CDATA[ >= ${start} ]]>                                       
    </select>



    
